<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dimfcompany.macapp2">

    <application

        android:allowBackup="true"
        android:icon="@drawable/maclogo256"
        android:label="@string/app_name"
        android:roundIcon="@drawable/maclogo256"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity_menu" android:screenOrientation="portrait"/>
        <activity
            android:name=".itemDetails"
            android:theme="@style/Activity_Dialog"
            android:screenOrientation="portrait"/>
        <activity android:name=".basket_screen" android:screenOrientation="portrait"/>
        <activity android:name=".login_register" android:screenOrientation="portrait"/>


    </application>

</manifest>